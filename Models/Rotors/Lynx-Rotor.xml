<PropertyList>


   
	<path>rotor.ac</path>
	
	<animation>
		<type>rotate</type>
		<object-name>rotor</object-name>
		<property>rotors/main/blade[0]/position-deg</property>
		<offset-deg>45</offset-deg>
		<axis>
			<x1-m>0.0</x1-m>
			<y1-m>0.0</y1-m>
			<z1-m>1.3</z1-m>
			<x2-m>-0.0</x2-m>
			<y2-m>0.0</y2-m>
			<z2-m>2.065</z2-m>
		</axis>
	</animation>
	
	
	 <animation>
    <type>spin</type>
    <object-name>rotor</object-name>
   <property>rotors/main/rpm</property>
		<factor>0.1085</factor>
    <center>
     <x1-m>0.0</x1-m>
			<y1-m>0.0</y1-m>
			<z1-m>1.62</z1-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y>  0.000 </y>
      <z>  1.000 </z>
    </axis>
  </animation>






</PropertyList>